<!--home.handlebars-->
<h1>Companies</h1>
{{#each companies}}
    <article>
        <a href={{companyUrl this.id}}>
            <img src={{this.logoUrl}} width="500"/>
            <h2>{{this.name}}</h2>
        </a>
        <button id={{deleteId this.id}}>Delete</button>
    </article>
    <br>
{{/each}}